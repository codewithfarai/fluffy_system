[bastion]
bastion-prod ansible_host=YOUR_BASTION_IP ansible_user=root

[managers]
manager-1 ansible_host=10.0.1.10 ansible_user=root
manager-2 ansible_host=10.0.1.11 ansible_user=root  
manager-3 ansible_host=10.0.1.12 ansible_user=root

[workers]
worker-1 ansible_host=10.0.1.20 ansible_user=root
worker-2 ansible_host=10.0.1.21 ansible_user=root
worker-3 ansible_host=10.0.1.22 ansible_user=root
worker-4 ansible_host=10.0.1.23 ansible_user=root
worker-5 ansible_host=10.0.1.24 ansible_user=root

[edge]
edge-1 ansible_host=10.0.1.30 ansible_user=root
edge-2 ansible_host=10.0.1.31 ansible_user=root

[swarm:children]
managers
workers
edge

[docker_nodes:children]
managers
workers
edge

[swarm:vars]
ansible_ssh_common_args='-o ProxyCommand="ssh -W %h:%p -q root@YOUR_BASTION_IP"'
docker_edition=ce
docker_channel=stable
docker_compose_version=2.21.0
swarm_init_advertise_addr=10.0.1.10
swarm_token_dir=/opt/docker/swarm-tokens

[managers:vars]
swarm_role=manager

[workers:vars]
swarm_role=worker

[edge:vars]
swarm_role=worker
node_labels='["traefik.enable=true", "edge=true"]'